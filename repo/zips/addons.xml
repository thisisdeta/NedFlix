<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="repository.nedflix" name="NedFlix Repository" version="1.0.0" provider-name="matke/deta">
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/thisisdeta/NedFlixSkin/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/thisisdeta/NedFlixSkin/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/thisisdeta/NedFlixSkin/master/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>NedFlix Repository</summary>
        <description>A repository for NedFlix skin.</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon><addon id="script.bingie.manage.widgets.fen.light" version="1.3.0" name="Bingie Manage Widgets (Fen Light)" provider-name="ChatGPDeta">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
    </requires>
    <extension point="xbmc.python.script" library="default.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Manage Bingie Widgets</summary>
        <description>An addon to help manage widgets within the Bingie skin more easily.</description>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>            
        </assets>
    </extension>
</addon></addons>